set cut_paste_input [stack 0]
push $cut_paste_input
NoOp {
 name New_Script_Generator
 label "\[value script_name]"
 note_font_size 15
 selected true
 addUserKnob {20 User}
 addUserKnob {1 proj l "Projects Path" +INVISIBLE}
 proj "\[python \{projects_dir\}]"
 addUserKnob {1 show l Show}
 show SHOW
 addUserKnob {1 seq l Sequence}
 seq SEQ
 addUserKnob {1 shot l Shot}
 shot SHOT
 addUserKnob {26 ""}
 addUserKnob {1 script_name l "Script Name"}
 addUserKnob {26 ""}
 addUserKnob {22 calc l "Calculate Script Name" T new_script_generator.calculate_script_name() +STARTLINE}
 addUserKnob {22 save_script l "Save Script" -STARTLINE T new_script_generator.save_script()}
}
